{
  "name": "spotify-artists-search",
  "version": "1.0.0",
  "description": "Search Spotify Artists and Albums",
  "author": "Elias Hourany",
  "private": true,
  "scripts": {
    "dev": "nuxt -o",
    "dev:e2e": "cypress open --mode=development",
    "build": "nuxt build --modern",
    "start": "nuxt start",
    "generate": "nuxt generate --modern",
    "build:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"**/*.{vue,scss}\"",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint": "run-s lint:all:*",
    "new": "hygen new"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env PRE_COMMIT=true lint-staged -r"
    }
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.8.4",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "3.0.0-beta.19",
    "nuxt": "2.10.2",
    "nuxt-i18n": "^6.0.1",
    "roboto-fontface": "^0.10.0"
  },
  "devDependencies": {
    "@mdi/font": "^4.6.95",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "@nuxtjs/vuetify": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.1",
    "cypress": "^3.4.1",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^3.0.3",
    "hygen": "^4.0.8",
    "lint-staged": "^9.2.1",
    "markdownlint-cli": "^0.17.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.9.3",
    "vuepress": "^1.0.3"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
